<script>
    // Sélectionne le lien correspondant dans la navbar
    const partenairesLink = document.querySelector('#partenaires-link');

    // Ajoute un événement de clic au lien
    partenairesLink.addEventListener('click', function(event) {
        // Empêche le comportement par défaut du clic sur un lien
        event.preventDefault();

        // Récupère la position de la section "Partenaires" dans la page
        const partenairesSection = document.querySelector('#port');
        const position = partenairesSection.offsetTop;

        // Fait défiler la page jusqu'à la position de la section "Partenaires"
        window.scrollTo({
            top: position,
            behavior: 'smooth'
        });
    });
</script>



